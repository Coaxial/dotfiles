[core]
  excludefiles = ~/.gitignore_global
  excludesfile = ~/.gitignore_global
  excludesfiles = ~/.gitignore_global
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path = 
[color]
  diff = auto
  status = auto
  branch = auto
[alias]
  co = checkout
  a = add
  cm = commit -m
  cam = commit -am
  s = status
  l = log --oneline --decorate --first-parent
  adall = add . --all
  edit = "!vim `git ls-files -m` -p"
  grep = grep -n
[push]
  default = simple
[include]
  path = ~/.gitconfig_local
